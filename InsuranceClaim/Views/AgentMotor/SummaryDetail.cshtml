                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-8 col-md-8 form-group-select">
                                            <div class="form-group">
                                                @Html.Label("Payment Type", new { @class = "gender-label" })
                                                @Html.HiddenFor(m => m.PaymentMethodId)
