<script>


    function Preview()
    {
        $("#divPreview").show();
        $("#myModal").modal("show")
    }

    function gotoExit() {
        $.ajax({
            url: "@Url.Action("gotoExit", "CustomerRegistration")",
            type: "Post",
            data: { id: @ViewBag.SummaryDetailId },
            success: function (data) {
                debugger;
                window.location.href = '/AgentMotor/CustomerDetail';
            }
        })
    }

    function PaymentMethod(paymentMethod)
    {
        debugger;
        $("li").removeClass("active");
        $("#payment_"+paymentMethod).addClass("active");
        $("#PaymentMethodId").val(paymentMethod);

        if(paymentMethod=="1")
        {
            $("#invoiceNumberDiv").show();
        }
        else
        {
            $("#invoiceNumberDiv").hide();
        }
    }


    function EditVehicleDetails(index) {
        var isUserStaff = '@User.IsInRole("AgentStaff")';
        var redirecturl = "";

        if(isUserStaff=='True')
        {
            redirecturl = '@Url.Action("RiskDetail", "AgentMotor", new { id= 0 })';
        }
        else
        {
            redirecturl = '@Url.Action("CustomerDetail", "AgentMotor", new { id= 0 })';
        }


        redirecturl = redirecturl.replace("0", index);
        location.href = redirecturl;


    }

    function DeleteVehicleDetails(index) {
        var obj = {};
        obj.index = index;

        $.ajax({
            url: "@Url.Action("DeleteVehicle", "CustomerRegistration")",
            data: obj,
            type: "POST",
            dataType: "json",
            success: function (result) {
                if (result) {
                    location.href = '@Url.Action("SummaryDetail", "CustomerRegistration")';
                }
            }
        });
    }

    function getVehicleList() {
        debugger;

        $.ajax({
            url: "@Url.Action("getVehicleList", "CustomerRegistration")",
            type: "GET",
            data: { summaryDetailId: @ViewBag.SummaryDetailId },
            dataType: "json",
            success: function (result) {
                var list = result;

                var bodyHTML = "";
                var count = 0;
                var totalpremium = 0.00;
                $.each(list, function (index, value) {
                    debugger;
                    count++;

                    var premiumWithDiscount=  parseFloat(value.premium) + parseFloat(value.Discount) ;

                    bodyHTML += "<tr><td>" + value.make + " "+ value.model+"(" + value.RegistrationNo+" )" + "</td><td>" + value.CurrencyName + "</td><td>" +  value.suminsured + "</td><td>" +   premiumWithDiscount.toFixed(2)   + "</td><td>" + value.radio_license_fee + "</td><td>" +  value.ZTSCLevy + "</td><td>" +  value.vehicle_license_fee+ "</td> <td>" +  value.stampDuty+ "</td><td>" + value.total+ "</td><td><div class='tbl-btn-list'><ul class='list-inline'><li><button type='button' onclick='EditVehicleDetails(" + count + ")' class = 'btn-sm btn-primary' >Edit</button></li><li><button type='button' onclick='DeleteVehicleDetails(" + count + ")' class='btn-sm btn-danger' >Delete</button></li></ul></div></td></tr>"
                    totalpremium += parseFloat(value.premium);
                });

                $("#tdTotalPremium").text("$" + totalpremium);

                $("#tblVehicleList tbody").append(bodyHTML);

                if (bodyHTML != "") {
                    $("#divVehicleList").css("display", "block");
                }
                else {
                    $("#divVehicleList").css("display", "none");
                }
            }
        });
    }
    function gotoPeriviouse() {
        // $("#previouselink")[0].click();
        debugger;
        var isUserStaff = '@User.IsInRole("Staff")';

        if(isUserStaff=='True')
        {
            window.location.href  = '@Url.Action("RiskDetail", "AgentMotor", new { id= 1 })';
        }
        else
        {
            window.location.href = '@Url.Action("RiskDetail", "AgentMotor", new { id= 1 })';
        }


    }
    function Replacable() {
        window.location.href = "@Url.Action("PaymentDetail", "CustomerRegistration")";
    }
    function SelectPlan(element) {
        debugger;
        $(".choose-plan-box").removeClass("active");
        var planPurchase = $(element).attr("planpurchase");
        $("#PaymentTermId").val(planPurchase);
        $(element).addClass("active");
    }

    function checkAmountPaid() {
        debugger;
        //var minAmount = $("#MinAmounttoPaid").val();
        //var maxAmount = $("#MaxAmounttoPaid").val();
        var Amount = $("#AmountPaid").val();
        var paymentMethod = $("#PaymentMethodId").val();

        var flag = false;
        if (Amount == "" && Amount == "0.00") {
            toastr.error("Please Enter Amount To Be Paid");
            return false;
        }
        else if(paymentMethod=="1")
        {
            if(confirm("Are you sure you want to proceed with CASH payment?"))
            {
                return true;
            }
            else{
                gotoExit();
            }
        }
        else{
            return true;
        }

        return false;
    }

    $(document).ready(function () {

        // IceCash payment
        //$('#btnSubmit').click(function() {
        //    $('#checkout-form').submit();
        //});

        //$('#btnDebug').click(function() {
        //    $('#hddebug').val('1');
        //    $('#checkout-form').submit();
        //});

        var isUserStaff = '@User.IsInRole("Staff")';
        var isRenewals = '@User.IsInRole("Renewals")';


        if('@Model.CustomSumarryDetilId'!="")
        {
            $("#invoiceNumberDiv").hide();
        }

        if(isRenewals)
        {
            $("#payment_1").addClass("active");
            $("#invoiceNumberDiv").show();
        }
      else  if(isUserStaff == "False")
        {
            $("#payment_2").addClass("active");
            $("#invoiceNumberDiv").hide();
        }
        else{
            $("#invoiceNumberDiv").show();
        }

        if ($("#sucessmsg").val()!="") {
            alert($("#sucessmsg").val());
            debugger;

            if('@userLoggedin'=='False')
            {
                location.href = '@Url.Action("Index", "InsuranceHome")';
            }

            location.href = '@Url.Action("QuotationList", "Account")';
        }

        if ($("#errMsg").val()=="Please enter invoice number.") {
            alert("Please enter invoice number.");
        }

        $("#datetimepicker1").datetimepicker({
            format: 'MM/D/YYYY',
            minDate: new Date()
        });



        getVehicleList();

        $("#PaymentMethodId").val('');

        if (@Model.PaymentMethodId != null && @Model.PaymentMethodId > 0)
        {
            $("#PaymentMethodId").val('@Model.PaymentMethodId');
        }

    });
</script>
