<script type="text/javascript">

    $(document).ready(function () { // will trigger when the document is ready
        $('#datetimepicker1').datetimepicker({
            format: 'MM/D/YYYY',
            maxDate: new Date()
        });

        $("#SummaryDetail_PaymentMethodId").val(3);

        $(function () {
            $("#btnSignup").click(function (e) {

                debugger;

                var paymentMethod = $("#SummaryDetail_PaymentMethodId").val();
                var paymentMethodName = "PayNow";
                if (paymentMethod == 2) {
                    paymentMethodName = "EcoCash";
                }

                var isConfirm = confirm("Do you want make payment from " + paymentMethodName + " ?");

                if (isConfirm) {
                    $(".loading-area").show();
                    $("#signupform").submit();
                } else {

                }
            });
        });

    });


    function PaymentMethod(paymentMethod) {
        $("#SummaryDetail_PaymentMethodId").val(paymentMethod)

        $("li").removeClass("active");
        $("#payment_" + paymentMethod).addClass("active");
    }

    function GetVehicleUsage() {
        $.ajax({
            url: "/CustomerRegistration/GetVehicleUsage",
            type: "POST",
            data: { ProductId: $("#RiskDetail_ProductId").val() },
            dataType: "json",
            success: function (result) {
                var $dropdown = $("#RiskDetail_VehicleUsage");
                $dropdown.empty();
                $.each(result, function () {
                    $dropdown.append($("<option />").val(this.Id).text(this.VehUsage));
                });
            }
        });
    }

</script>


